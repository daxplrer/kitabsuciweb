mixin addfooter()
    footer.d-flex.flex-wrap.justify-content-between.align-items-center.py-3.my-4.border-top.rounded-top.mx-5
        .col-md-4.d-flex.align-items-center
            a.mb-3.me-2.mb-md-0.text-muted.text-decoration-none.lh-1(href="https://daxplrer.github.io")
                i.bi.bi-compass(style="font-size: 45px")
            span.text-muted Buatan daxplrer, buat tugas PBM
        ul.nav.col-md-4.justify-content-end.list-unstyled.d-flex
            li.ms-3
                a.text-muted(href="https://github.com/daxplrer")
                    i.bi.bi-github(style="font-size: 50px")
            li.ms-3
                a.text-muted(href="https://npmjs.com/~daxplrer")
                    i.bi.bi-box-seam-fill(style="font-size: 50px")
            li.ms-3
                a.text-muted(href="https://instagram.com/daxplrer.public")
                    i.bi.bi-instagram(style="font-size: 50px")